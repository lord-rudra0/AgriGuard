    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
  "VITE_API_URL": "https://agriguard-vgpa.onrender.com/",
  "VITE_SOCKET_URL": "https://agriguard-vgpa.onrender.com/"
  }
}
